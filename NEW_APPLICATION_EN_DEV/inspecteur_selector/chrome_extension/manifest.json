{
  "manifest_version": 3,
  "name": "Selector Inspector",
  "version": "0.1",
  "description": "Send CSS selector or XPath to the PySide app via WebSocket.",
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
